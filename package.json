{
  "name": "@liedsonc/core-auth-kit",
  "version": "2.2.0",
  "description": "Production-ready authentication UI package for Next.js App Router",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./styles": "./styles/index.css"
  },
  "bin": {
    "core-auth-kit": "./bin/init.js",
    "@liedsonc/core-auth-kit": "./bin/init.js"
  },
  "files": [
    "dist",
    "styles",
    "bin",
    "templates"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "type-check": "tsc --noEmit",
    "publish": "npm publish --access public",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.0",
    "lucide-react": "^0.460.0"
  },
  "peerDependencies": {
    "next": "^15.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "typescript": "^5.6.0"
  },
  "keywords": [
    "nextjs",
    "auth",
    "authentication",
    "ui",
    "shadcn",
    "react",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liedsonc/core-auth-kit.git",
    "directory": "auth-ui"
  },
  "bugs": {
    "url": "https://github.com/liedsonc/core-auth-kit/issues"
  },
  "homepage": "https://github.com/liedsonc/core-auth-kit#readme"
}